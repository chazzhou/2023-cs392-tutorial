{
  "name": "vite-template-react",
  "version": "1.2.0",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "cy:open": "cypress open",
    "em:start": "firebase emulators:start --import=./saved-data --export-on-exit",
    "em:exec": "firebase emulators:exec --import=./saved-data 'npm start'",
    "em:execui": "firebase emulators:exec --ui --import=./saved-data 'npm start'",
    "em:cy:open": "start-server-and-test em:exec http://127.0.0.1:3000 cy:open"
  },
  "dependencies": {
    "@nextui-org/react": "^2.1.13",
    "firebase": "^10.4.0",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.3",
    "autoprefixer": "^10.4.16",
    "c8": "^8.0.1",
    "cypress": "^13.4.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.30",
    "start-server-and-test": "^2.0.1",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  }
}
